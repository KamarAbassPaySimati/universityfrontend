  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    // "eslint": "eslint src/**/*.jsx src/**/**/*.jsx src/**/**/**/*.jsx src/*.jsx src/**/*.js src/**/**/*.js src/**/**/**/*.js test/**/*.js test/**/**/*.js --fix",
