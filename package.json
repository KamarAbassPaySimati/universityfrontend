{
  "name": "paymaart-admin-web-application",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite --host",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "eslint": "eslint src/**/*.jsx src/**/**/*.jsx src/**/**/**/*.jsx src/*.jsx src/**/*.js src/**/**/*.js src/**/**/**/*.js test/**/*.js test/**/**/*.js --fix",
    "bdd_dry_run": "./node_modules/@cucumber/cucumber/bin/cucumber.js --dry-run test --require test",
    "generate_report": "nyc report --temp-dir ./test/step-definations/coverageData --reporter html --report-dir ./coverage",
    "code_coverage": "nyc check-coverage --temp-dir ./test/step_definitions/coverageData",
    "bdd_unit": "./node_modules/@cucumber/cucumber/bin/cucumber.js --import test --force-exit -f json:./reports/test-report-unit-test-${featureFile}.json test/unit-testing/login.feature",
    "owasp": "owasp-dependency-check --out=./dependency-check-reports --project ARCM -f JSON -f HTML --prettyPrint --failOnCVSS 8 --data=/tmp/dependency-check-data --scan=package-lock.json --log=logs.txt && cat logs.txt"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.2.1",
    "antd": "^5.16.1",
    "assert": "^2.1.0",
    "aws-amplify": "^6.0.28",
    "base32-decode": "^1.0.0",
    "chromedriver": "^123.0.1",
    "country-list": "^2.3.0",
    "crypto": "^1.0.1",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "framer-motion": "^11.0.8",
    "jsonwebtoken": "^9.0.2",
    "jsqr": "^1.4.0",
    "lottie-react": "^2.4.0",
    "nyc": "^15.1.0",
    "qrcode.react": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-google-autocomplete": "^2.7.3",
    "react-paginate": "^8.2.0",
    "react-redux": "^9.1.1",
    "react-responsive-modal": "^6.4.2",
    "react-router": "^6.22.3",
    "react-router-dom": "^6.22.3",
    "react-spinners": "^0.13.8",
    "react-tooltip": "^5.26.3",
    "react-webcam": "^7.2.0",
    "rollup": "^4.13.0",
    "uuid": "^9.0.1"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.3.1",
    "@faker-js/faker": "^8.4.1",
